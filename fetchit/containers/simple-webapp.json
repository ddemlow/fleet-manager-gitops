{
  "name": "simple-webapp",
  "image": "docker.io/mmumshad/simple-webapp-color:latest",
  "ports": [
    {
      "hostPort": 9090,
      "containerPort": 8080,
      "protocol": "tcp"
    }
  ],
  "environment": {
    "APP_COLOR": "blue",
    "APP_NAME": "GitOps Demo App"
  },
  "labels": {
    "app": "simple-webapp",
    "managed-by": "fetchit",
    "source": "gitops"
  },
  "restartPolicy": "unless-stopped"
}
