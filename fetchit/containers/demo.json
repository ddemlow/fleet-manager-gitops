{
  "name": "demo-web",
  "image": "nginx:alpine",
  "ports": [
    {
      "hostPort": 8080,
      "containerPort": 80,
      "protocol": "tcp"
    }
  ],
  "volumes": ["/var/edge/www:/usr/share/nginx/html:ro"],
  "environment": {
    "NGINX_HOST": "localhost",
    "NGINX_PORT": "80"
  },
  "restart": "always"
}
