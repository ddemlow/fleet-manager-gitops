{
  "name": "demo-web",
  "image": "nginx:alpine",
  "ports": [
    {
      "hostPort": 8080,
      "containerPort": 80,
      "protocol": "tcp"
    }
  ],
  "volumes": [
    {
      "source": "/var/edge/www",
      "destination": "/usr/share/nginx/html",
      "options": "ro"
    }
  ],
  "environment": {
    "NGINX_HOST": "localhost",
    "NGINX_PORT": "80"
  },
  "restart": "always"
}
