{
  "Image": "nginx:alpine",
  "Name": "demo-web",
  "Env": {
    "NGINX_HOST": "localhost",
    "NGINX_PORT": "80"
  },
  "Mounts": [{
    "Source": "/var/www",
    "Destination": "/usr/share/nginx/html"
  }],
  "Ports": [{
    "host_ip": "",
    "container_port": 80,
    "host_port": 8080,
    "range": 0,
    "protocol": "tcp"
  }]
}
